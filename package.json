		"bcrypt-nodejs": "0.0.3",
		"body-parser": "^1.18.2",
		"concurrently": "^3.5.1",
		"cookie-parser": "~1.4.3",
		"debug": "~2.6.9",
		"ejs": "~2.5.7",
		"express": "~4.16.0",
		"express-mailer": "^0.3.1",
		"http-errors": "~1.6.2",
		"jsonwebtoken": "^8.2.1",
