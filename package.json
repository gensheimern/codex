{
	"name": "lunch-planner-backend",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"installclient": "cd frontend && npm install",
		"start": "nodemon server",
		"server": "nodemon server.js",
		"client": "npm start --prefix frontend",
		"all": "concurrently \"npm start\" \"npm start --prefix frontend\" --kill-others",
		"build": "npm run build --prefix frontend",
		"prod": "npm run build --prefix frontend & npm start",
		"test": "mocha",
		"test:coverage": "nyc mocha --timeout=3000",
		"testclient": "npm run test --prefix frontend",
		"testclient:coverage": "npm run test:coverage --prefix frontend"
	},
	"engines": {
		"node": ">=6.9.1"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/gensheimern/codex.git"
	},
	"bugs": {
